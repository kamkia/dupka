#include <iostream>

using namespace std;

void gwiazdki ()
{
    int znak;
    std::cout << "wpisz znak z klawiatury" << std::endl;
    for (int i=0; i<10; i++)
        
        cin >> znak;
        cout << znak << std::endl;
        
}

int main()
{
    int znak;
    std::cout << gwiazdki(znak);

    return 0;
}
